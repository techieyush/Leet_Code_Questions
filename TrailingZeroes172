
import java.util.Scanner;

public class TrailingZeros172 {
    public static int countZeros(int n)
    {
        int count=0;
        while(n>0)
        {
            n/=5;
            count+=n;
        }
        return count;

    }
    public static void main(String[]args)
    {
        Scanner sc= new Scanner(System.in);
        int n=sc.nextInt();
        System.out.println(countZeros(n));
       
    }
    
}
