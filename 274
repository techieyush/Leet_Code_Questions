
class Solution {
    public int hIndex(int[] citations) {
        Arrays.sort(citations);
        int res=0;
        int n=citations.length;
        for(int i=0;i<n;i++)
        {
            if(n-i<=citations[i])
            {
                res=Math.max(res,n-i);
            }
        }
        return res;
        
    }
}
